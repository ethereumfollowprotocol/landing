{
  "name": "efp-landing",
  "version": "0.0.0",
  "description": "EFP Landing Page",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "format": "biome format . --write",
